<xml>
    <block type="start" deletable="false" x="30" y="30">
        <next>
            <block type="controls_repeat">
                <field name="TIMES">2</field>
                <statement name="DO">
                    <block type="move_forwards"></block>
                </statement>
                <next>
                    <block type="turn_left">
                        <next>
                            <block type="turn_right">
                                <next>
                                    <block type="controls_repeat">
                                        <field name="TIMES">2</field>
                                        <statement name="DO">
                                            <block type="turn_right">
                                                <next>
                                                    <block type="turn_left"></block>
                                                </next>
                                            </block>
                                        </statement>
                                        <next>
                                            <block type="move_forwards">
                                                <next>
                                                    <block type="turn_left"></block>
                                                </next>
                                            </block>
                                        </next>
                                    </block>
                                </next>
                            </block>
                        </next>
                    </block>
                </next>
            </block>
        </next>
    </block>
</xml>